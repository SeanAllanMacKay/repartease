{
  "expo": {
    "name": "repartease",
    "slug": "repartease",
    "version": "1.0.0",
    "scheme": "repartease",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#aa00a3"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "buildNumber": "1"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#aa00a3"
      },
      "package": "com.seanmackay.repartease",
      "versionCode": 1
    },
    "web": {
      "output": "single",
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "63453c7b-2733-40bc-a016-6c240f2d8a45"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": ["./assets/karmatic_arcade.ttf"]
        }
      ]
    ],
    "experiments": {
      "tsconfigPaths": true
    }
  }
}
